   "source": [
    "#Canny Edge detection\n",
    "\n",
    "import cv2\n",
    "\n",
    "image = cv2.imread('img/sst.jpg', cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "image = cv2.resize(image, (400,400))\n",
    "\n",
    "edges = cv2.Canny(image, (100, 200))\n",
    "\n",
    "cv2.imshow('Original', image)\n",
    "cv2.imshow('Edges', edges)\n",
    "\n",
    "cv2.wait"
   ]
  }
